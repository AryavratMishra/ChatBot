{
  "version": 2,
  "builds": [
    { "src": "api/gemini.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" },
    { "src": "src/main.jsx", "use": "@vercel/static" }
  ],
  "routes": [
    { "src": "/api/gemini", "dest": "/api/gemini.js" },
    { "src": "/(.*)", "dest": "/" }
  ]
}
